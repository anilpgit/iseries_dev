
CREATE OR REPLACE TABLE INVIINF (
  INVITM_INFO_ID FOR COLUMN ARID       CHAR(6)
     CCSID 297 NOT NULL DEFAULT '' ,
  INVITM_INFORMATION FOR COLUMN ARTINF     VARCHAR(1520)
      CCSID 297 NOT NULL DEFAULT '' )
  ;

LABEL ON TABLE INVIINF
  IS 'Inv. Item Informations File' ;

LABEL ON COLUMN INVIINF
( INVITM_INFO_ID IS 'INV. ITEM ID' ,
  INVITM_INFORMATION IS 'INV. ITEM INFORMATION' ) ;

